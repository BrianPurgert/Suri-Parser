	.content{
		left:        0;
		right:       0;
		min-height:  100vh;
		margin:      10vh auto;
		perspective: 2px;
		height:      220vh;
		position:    absolute;
		top:         0;
		z-index:     1;
	}

	.parallax{
		/* 	background-color: rgba(48, 241, 255, 0.9); */
		perspective: 1px;
		height:      220vh;
		overflow-x:  hidden;
		overflow-y:  auto;
		position:    relative;
	}

	.object-wrapper{
		position:        absolute;
		top:             0;
		right:           0;
		bottom:          0;
		left:            0;
		background:      none;
		justify-content: center;
	}

	.elevation-16{
		box-shadow: 0 8px 10px -5px rgba(0, 0, 0, 0.2),
		            0 16px 24px 2px rgba(0, 0, 0, 0.14), 0 6px 30px 5px rgba(0, 0, 0, 0.12);
	}

	.stripe{
		height: 120vh;
		width:  120vw;
		left:   0;
		top:    0;
	}

	.stripe-1{
		transform:   rotatez(-15deg);
		margin-top:  20vh;
		margin-left: 10vh;
		background:  #f4a113;
		animation:   stipe-1 30s ease infinite;
	}

	@keyframes stipe-1{
		0%{
			opacity:   1;
			transform: rotatez(-15deg) scale(1);
		}
		20%{
			opacity:   1;
			transform: rotatez(-25deg) scale(1.3);
		}
		80%{
			opacity:   1;
			transform: rotatez(-35deg) scale(1);
		}
		100%{
			opacity:   1;
			transform: rotatez(-15deg);
		}
	}

	.stripe-2{
		transform:   rotatez(50deg);
		margin-top:  40vh;
		margin-left: -100vh;
		background:  #bd2c44;
		background:  #212121;
		/* 	background:#2973a5; */
		animation:   stipe-2 16s ease infinite;
	}

	@keyframes stipe-2{
		0%{
			opacity:   1;
			transform: rotatez(50deg) rotatex(0deg) rotatey(0deg) scale(1);
		}
		60%{
			opacity:   1;
			transform: rotatez(20deg) rotatex(0deg) rotatey(0deg) scale(1.2);
		}
		100%{
			opacity:   1;
			transform: rotatez(50deg) rotatex(0deg) rotatey(0deg) scale(1);
		}
	}

	.stripe-3{
		transform:   rotatez(35deg);
		margin-top:  70vh;
		margin-left: -100vh;
		/* 		background:#bd2c44; */
		/* 	background:#212121; */
		background:  #2973a5;
		animation:   stipe-3 16s ease infinite;
	}

	@keyframes stipe-3{
		0%{
			opacity:   1;
			transform: rotatez(35deg) rotatex(0deg) scale(1);
		}
		60%{
			opacity:   1;
			transform: rotatez(35deg) rotatex(0deg) scale(1.2);
		}
		100%{
			opacity:   1;
			transform: rotatez(35deg) rotatex(0deg) scale(1);
		}
	}

	.stripe-4{
		transform:   rotateX(0deg) rotatez(60deg);
		margin-top:  60vh;
		margin-left: 40vh;
		background:  rgb(189, 44, 68);
		/*background-color: #cc4c4200!important;*/

		/* 	background:#212121; */
		/* 	background:#2973a5; */
		animation:   stipe-4 5s ease-in-out infinite;
	}

	@keyframes stipe-4{
		0%{
			opacity:   1;
			transform: rotateX(0deg) rotatez(60deg) scale(1);
		}
		80%{
			opacity:   1;
			transform: rotateX(0deg) rotatez(50deg) scale(1);
		}
		100%{
			opacity:   1;
			transform: rotateX(0deg) rotatez(60deg) scale(1);
		}
	}

	.stripe-5{
		transform:   rotatez(45deg);
		margin-top:  60vh;
		margin-left: 90vh;
		background:  #bd2c44;
		background:  #212121;
		/* 	background:#2973a5; */
	}

	.speed-1{ transform: translateZ(-2px) scale(3); }

